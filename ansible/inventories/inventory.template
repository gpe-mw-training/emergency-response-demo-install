[all:vars]
# MapBox API token, see https://docs.mapbox.com/help/how-mapbox-works/access-tokens/
map_token=replaceme

# valid choices: image, source
deploy_from=image

# If set to True, then every user that creates an account in Emergency Response demo will automatically be a "realm-admin"
set_realm_admin_as_default=False

# If set to true, installs the monitoring stack
install_monitoring=true

# If set to true, installs the tools (pgadmin)
install_tools=true

# SSO theme extension
emergency_response_theme_extension=https://github.com/Emergency-Response-Demo/emergency-sso/releases/download/v1.1.0/emergency-sso-theme-1.1.0.jar

